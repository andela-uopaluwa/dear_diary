<h1>Entries</h1>
<% @daily_entries.each do |daily_entry| %>
  <div class="container">
    <p>
      <%= daily_entry.prefix %><br>
      <%= daily_entry.body[0..30] %>....
      <%= link_to 'Show', daily_entry %>
      <%= link_to 'Edit', edit_daily_entry_path(daily_entry) %>
      <%= link_to 'Delete', daily_entry,
        method: :delete ,
        data: { confirm: 'Are you sure?' } %>
    </p>
  </div>
<% end %>